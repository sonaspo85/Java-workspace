<?xml version="1.0"?>
<font-mappings>
	<font-table>
		<!-- Backwards compatibility aliases -->
		<aliases>
			<alias name="serif">Serif</alias>
		</aliases>
		<aliases>
			<alias name="sans-serif">Sans</alias>
		</aliases>
		<aliases>
			<alias name="monospace">Monospaced</alias>
		</aliases>
		<aliases>
			<alias name="Helvetica">Normal</alias>
		</aliases>
		<aliases>
			<alias name="Courier">Monospaced</alias>
		</aliases>
		<aliases>
			<alias name="minchan">Minchan</alias>
		</aliases>

		<logical-font name="Minchan">
			<physical-font char-set="default">
				<font-face>SamsungOne-700, Anonymous Pro, Arial Unicode MS</font-face>
			</physical-font>
			<!-- <physical-font char-set="Simplified Chinese">
				<font-face>Anonymous Pro, AdobeSongStd-Light, Arial Unicode MS, Batang, SimSun</font-face>
			</physical-font>
			<physical-font char-set="Japanese">
				<font-face>Anonymous Pro, KozMinProVI-Regular, Arial Unicode MS, Batang</font-face>
			</physical-font>
			<physical-font char-set="Korean">
				<font-face>Anonymous Pro, AdobeMyungjoStd-Medium, Arial Unicode MS, Batang</font-face>
			</physical-font>
			<physical-font char-set="Symbols">
				<font-face>ZapfDingbats</font-face>
			</physical-font>
			<physical-font char-set="SubmenuSymbol">
				<font-face>ZapfDingbats</font-face>
			</physical-font>
			<physical-font char-set="SymbolsSuperscript">
				<font-face>Anonymous Pro, Trebuchet MS, Helvetica, Arial Unicode MS</font-face>
				<baseline-shift>20%</baseline-shift>
				<override-size>smaller</override-size>
			</physical-font> -->
		</logical-font>

		<logical-font name="Sans">
			<physical-font char-set="default">
				<font-face>Trebuchet MS, Helvetica, Arial Unicode MS</font-face>
			</physical-font>
			<physical-font char-set="Simplified Chinese">
				<font-face>AdobeSongStd-Light, Arial Unicode MS, Batang, SimSun</font-face>
			</physical-font>
			<physical-font char-set="Japanese">
				<font-face>KozMinProVI-Regular, Arial Unicode MS, Batang</font-face>
			</physical-font>
			<physical-font char-set="Korean">
				<font-face>AdobeMyungjoStd-Medium, Arial Unicode MS, Batang</font-face>
			</physical-font>
			<physical-font char-set="Symbols">
				<font-face>ZapfDingbats</font-face>
			</physical-font>
			<physical-font char-set="SubmenuSymbol">
				<font-face>ZapfDingbats</font-face>
			</physical-font>
			<physical-font char-set="SymbolsSuperscript">
				<font-face>Trebuchet MS, Helvetica, Arial Unicode MS</font-face>
				<baseline-shift>20%</baseline-shift>
				<override-size>smaller</override-size>
			</physical-font>
		</logical-font>

		<logical-font name="Serif">
			<physical-font char-set="default">
				<font-face>SamsungOne-700, Book Antiqua, Times New Roman, Times, Arial Unicode MS, Tahoma, Batang, SimSun</font-face>
			</physical-font>
			<physical-font char-set="Simplified Chinese">
				<font-face>AdobeSongStd-Light, Arial Unicode MS, Batang, SimSun</font-face>
			</physical-font>
			<physical-font char-set="Japanese">
				<font-face>KozMinProVI-Regular, Arial Unicode MS, Batang</font-face>
			</physical-font>
			<physical-font char-set="Korean">
				<font-face>AdobeMyungjoStd-Medium, Arial Unicode MS, Batang</font-face>
			</physical-font>
			<physical-font char-set="Symbols">
				<font-face>ZapfDingbats</font-face>
			</physical-font>
			<physical-font char-set="SubmenuSymbol">
				<font-face>ZapfDingbats</font-face>
			</physical-font>
			<physical-font char-set="SymbolsSuperscript">
				<font-face>SamsungOne-700, Times New Roman, Times, Arial Unicode MS, Tahoma, Batang</font-face>
				<baseline-shift>20%</baseline-shift>
				<override-size>smaller</override-size>
			</physical-font>
		</logical-font>

		<logical-font name="Monospaced">
			<physical-font char-set="default">
				<font-face>Consolas, Courier New, Courier</font-face>
			</physical-font>
			<physical-font char-set="Simplified Chinese">
				<font-face>AdobeSongStd-Light, Arial Unicode MS, Batang, SimSun</font-face>
			</physical-font>
			<physical-font char-set="Japanese">
				<font-face>KozMinProVI-Regular, Arial Unicode MS, Batang</font-face>
			</physical-font>
			<physical-font char-set="Korean">
				<font-face>AdobeMyungjoStd-Medium, Arial Unicode MS, Batang</font-face>
			</physical-font>
			<physical-font char-set="Symbols">
				<font-face>ZapfDingbats</font-face>
			</physical-font>
			<physical-font char-set="SymbolsSuperscript">
				<font-face>Courier New, Courier, Arial Unicode MS, Tahoma, Batang</font-face>
				<baseline-shift>20%</baseline-shift>
				<override-size>smaller</override-size>
			</physical-font>
		</logical-font>



	</font-table>

</font-mappings>