<project name="com.ast.protean.ma.um2020pdf" xmlns:if="ant:if" xmlns:unless="ant:unless">
    <property name="transtype" value="protean_ma_um2020pdf" />
    <property name="clean.temp" value="false" />
    <property name="dita.temp.dir" location="${output.dir}\temp" />
    <!-- <property name="args.debug" value="yes"/> -->
    <property name="args.draft" value="no" />
    <target name="dita2protean_ma_um2020pdf" depends="protean_ma_um2020pdf_main" />
    <target name="protean_ma_um2020pdf_main" xmlns:dita="http://dita-ot.sourceforge.net" dita:extension="depends org.dita.dost.platform.InsertDependsAction" depends="clean.samples_pdf">
        <property name="customization.dir" location="${dita.plugin.com.ast.protean.ma.um2020pdf.dir}${file.separator}cfg" />
        <basename property="outputname" file="${args.input}" suffix=".dita" />
        <property name="pdf.formatter" value="ah" />
        <property name="axf.path" value="z:" />
        <!-- <property name="axf.path" value="C:\AHFormatterV63"/>  -->
        <property name="images.dir" location="${output.dir}${file.separator}..${file.separator}images" />
        <property name="pdf2.i18n.skip" value="false" />
        <antcall target="dita2pdf2" />
        <move file="${output.dir}\${outputname}.pdf" todir="${output.dir}\1_PDF" overwrite="yes" />
        <copy todir="${output.dir}\1_PDF" overwrite="yes">
            <fileset dir="${dita.temp.dir}" includes="topic.fo" />
        </copy>
    </target>
    <target name="clean.samples_pdf" description="remove the sample PDF output">
        <!-- <delete dir="${output.dir}${file.separator}1_PDF"/> -->
        <!-- <mkdir dir="${output.dir}${file.separator}1_PDF" /> -->
        <!-- <echo message="dita2protean_ma_um2020pdf_${lgs}" /> -->
    </target>
</project>